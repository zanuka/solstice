{
  "name": "solstice",
  "version": "0.0.11",
  "description": "Simple Date Time picker",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonVeselka/solstice.git"
  },
  "author": {
    "name": "Jason Veselka",
    "url": "https://github.com/JasonVeselka",
    "email": "jason.veselka@live.com"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel -d ./lib ./src",
    "demo": "open html/index.html",
    "pretest": "npm run build",
    "test": "mocha --reporter spec --full-trace test/index.js",
    "test-ci": "npm run build",
    "coveralls": "coveralls < resport-lcov/**/*lcov.info",
    "postcoveralls": "rimraf coverage",
    "prepublish": "npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "keywords": [
    "date",
    "time",
    "picker",
    "calendar",
    "vanillaJS",
    "babel6",
    "es6"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-istanbul": "0.6.x",
    "babel-istanbul-loader": "0.0.1",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.2.0",
    "coveralls": "2.11.4",
    "loader-utils": "0.2.12",
    "mocha": "2.3.4",
    "rimraf": "2.5.0",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19"
  }
}
